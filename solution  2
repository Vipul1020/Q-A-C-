#include <iostream>
using namespace std ;
int main ()
{ 
    int a , b , temp ;
    cout<<"enter two numbers\n" ;
    cin>>a>>b;
    cout<<"Value of a is = "<<a<<endl; 
    cout<<"Value of b is = "<<b<<endl;
    temp = a ;
    a = b;
    b = temp;
    cout<<"after swapping\n";
    cout<<"Value of a is = "<<a<<endl;
    cout<<"Value of b is = "<<b<<endl;
    return 0 ;
}
